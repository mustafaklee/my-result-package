<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Result Package</Title>
    <Version>1.0.0</Version>
    <Authors>Mustafa Kale</Authors>
    <Company>Mustafa Kale</Company>
    <Product>MyResult</Product>
    <Description>Basic Result Structure API's</Description>
    <PackageIcon>flowchart.png</PackageIcon>
    <RepositoryUrl>https://github.com/mustafaklee/my-result-package.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>result;0 api-result; dataResult ;SuccesResult; ErrorResult</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\Downloads\flowchart.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FluentAssertions" Version="8.5.0" />
    <PackageReference Include="xunit.extensibility.core" Version="2.9.3" />
  </ItemGroup>

</Project>
